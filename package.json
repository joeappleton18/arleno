    "@mdx-js/react": "^1.6.16",
    "@mdx-js/tag": "^0.20.3",
    "@next/mdx": "^9.5.2",
    "axios": "^0.20.0",
    "cloudinary-react": "^1.6.6",
    "clsx": "^1.1.1",
    "del": "^5.1.0",
    "firebase": "^7.21.1",
    "firebase-admin": "^8.9.0",
    "firebase-functions": "^3.3.0",
    "immer": "^7.0.9",
    "next": "^9.3.4",
    "next-transpile-modules": "^4.1.0",
